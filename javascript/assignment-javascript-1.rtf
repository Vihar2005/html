        <h2>simple Arithmetic Calculator</h2> <hr>\par
        <input type="number" placeholder="Enter Any Number" id="num1">\par
        <br>\par
        <input type="number" placeholder="Enter Any Number" id="num2">\par
        <br>\par
        <button id="add">+</button>\par
        <button id="sub">-</button>\par
        <button id="mul">*</button>\par
        <button id="div">/</button><br>\par
        <input type="number" readonly id="result">\par
    </div>\par
    <script>\par
        var n1 = document.getElementById("num1");\par
        var n2 = document.getElementById("num2");\par
        var res = document.getElementById("result");\par
        document.getElementById("add").addEventListener("click", function() \{\par
            res.value = parseInt(n1.value)+parseInt(n2.value);\par
        \});\par
        document.getElementById("sub").addEventListener("click", function() \{\par
            res.value = parseInt(n1.value)-parseInt(n2.value);\par
        \});\par
        document.getElementById("mul").addEventListener("click", function() \{\par
            res.value = parseInt(n1.value)*parseInt(n2.value);\par
        \});\par
        document.getElementById("div").addEventListener("click", function() \{\par
            res.value = parseInt(n1.value)/parseInt(n2.value);\par
        \});\par
    </script>\par
